<p id="notice"><%= notice %></p>

<p>
  <strong>Преподаватель:</strong>
  <%= @cla.professor_id %>
</p>

<p>
  <strong>Дисциплина:</strong>
  <%= @cla.course_id %>
</p>

<p>
  <strong>Группы:</strong>
  <dd> <%= @cla.groups.map(&:name).join(', ') %></dd>
</p>

<p>
  <strong>Продолжительность занятия:</strong>
  <%= @cla.duration %>
</p>

<p>
  <strong>Лабораторная?:</strong>
  <%= @cla.lab %>
</p>

<%= link_to 'Редактировать', edit_cla_path(@cla) ,  class: "btn btn-mini btn-success" %> 
<%= link_to 'Назад', clas_path,  class: "btn btn-mini btn-default" %>
